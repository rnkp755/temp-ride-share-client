{
  "expo": {
    "name": "rnkp755-rideshare",
    "slug": "rnkp755-rideshare",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "@react-native-firebase/app",
      "@react-native-firebase/messaging"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.rnkp755.rideshare",
      "googleServicesFile": "./google-services.json",
      "notification": {
        "icon": "./assets/images/icon.png",
        "color": "#ffffff"
      }
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "cbc6816e-ad98-4e07-8fad-8d16a78323c3"
      }
    }
  }
}
